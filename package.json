{
  "name": "uniprof",
  "version": "0.3.3",
  "author": "Indragie Karunaratne <i@indragie.com>",
  "description": "Universal CPU profiler designed for humans and AI agents",
  "type": "module",
  "mcpName": "io.github.indragiek/uniprof",
  "main": "dist/index.js",
  "bin": {
    "uniprof": "./dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsgo -p tsconfig.json && cp -r speedscope dist/",
    "dev": "node dist/index.js",
    "start": "node dist/index.js",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "tsgo --noEmit",
    "test": "bun test --timeout 6000000"
  },
  "keywords": [
    "profiling",
    "profiler",
    "performance",
    "perf"
  ],
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.17.5",
    "@types/node": "24.3.1",
    "chalk": "5.6.2",
    "commander": "14.0.0",
    "execa": "9.6.0",
    "fast-xml-parser": "5.2.5",
    "ora": "8.2.0",
    "table": "6.9.0",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@types/table": "6.3.2",
    "@typescript/native-preview": "^7.0.0-dev.20250908.1",
    "typescript": "5.9.2"
  }
}
